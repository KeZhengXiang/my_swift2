
platform :ios, '9.0'

target 'newTest' do
  use_frameworks!

  #网络
  pod 'Alamofire'
  pod 'SwiftyJSON' 
  pod 'HandyJSON'

  #布局
  pod 'SnapKit'

  #网络图片
  pod 'Kingfisher'

  #上拉加载 下拉刷新
  pod 'MJRefresh'
  
  #数据库
  pod 'SQLite.swift'
  
  
  ######高德地图sdk#######
  pod 'AMap3DMap', '~>7.5.0' #3D地图SDK
  pod 'AMapSearch', '~>7.3.0' #地图SDK搜索功能
  pod 'AMapLocation', '~>2.6.5' #定位SDK
  
  
  pod 'AlipaySDK-iOS', '~>15.7.4' #支付宝支付SDK

end
